<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/summoner1"
        android:layout_width="311dp"
        android:layout_height="44dp"
        android:text="소환사1"
        app:backgroundTint="#D85656"
        app:layout_constraintBottom_toTopOf="@+id/summoner2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editSummonerName"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/summoner2"
        android:layout_width="311dp"
        android:layout_height="44dp"
        android:text="소환사2"
        app:backgroundTint="#D85656"
        app:layout_constraintBottom_toTopOf="@+id/summoner3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/summoner3"
        android:layout_width="311dp"
        android:layout_height="44dp"
        android:text="소환사3"
        app:backgroundTint="#D85656"
        app:layout_constraintBottom_toTopOf="@+id/summoner4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/summoner4"
        android:layout_width="311dp"
        android:layout_height="44dp"
        android:text="소환사4"
        app:backgroundTint="#D85656"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="219dp" />

    <Button
        android:id="@+id/summoner5"
        android:layout_width="311dp"
        android:layout_height="44dp"
        android:text="소환사5"
        app:backgroundTint="#D85656"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/summoner4"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/summoner6"
        android:layout_width="311dp"
        android:layout_height="44dp"
        android:text="소환사6"
        app:backgroundTint="#D85656"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/summoner5"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/summoner7"
        android:layout_width="311dp"
        android:layout_height="44dp"
        android:text="소환사7"
        app:backgroundTint="#D85656"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/summoner6"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/summoner8"
        android:layout_width="311dp"
        android:layout_height="44dp"
        android:text="소환사8"
        app:backgroundTint="#D85656"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/summoner7"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/summoner9"
        android:layout_width="311dp"
        android:layout_height="44dp"
        android:text="소환사9"
        app:backgroundTint="#D85656"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/summoner8"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/summoner10"
        android:layout_width="311dp"
        android:layout_height="44dp"
        android:text="소환사10"
        app:backgroundTint="#D85656"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/summoner9"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/add"
        android:layout_width="40dp"
        android:layout_height="47dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:text="+"
        android:textSize="20dp"
        app:backgroundTint="#AE3838"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.426"
        app:layout_constraintStart_toEndOf="@+id/editSummonerName"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.028"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start"
        app:backgroundTint="#AE3838"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.845"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/summoner10"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editSummonerName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="소환사 이름을 입력하세요"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.028"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>